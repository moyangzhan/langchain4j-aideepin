<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.moyz.adi.common.mapper.KnowledgeBaseQaRecordRefGraphMapper">
    <select id="listByQaUuid" resultType="com.moyz.adi.common.entity.KnowledgeBaseQaRecordRefGraph">
        select *
        from adi_knowledge_base_qa_record_ref_graph a
                 inner join adi_knowledge_base_qa_record b on a.qa_record_id = b.id
        where b.uuid = #{qaUuid}
    </select>

</mapper>
